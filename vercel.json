{
    "version": 2,
    "buildCommand": "pnpm build:prod",
    "installCommand": "pnpm install",
    "devCommand": "pnpm start",
    "outputDirectory": "dist",
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/main.ts",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
      }
    ]
  }